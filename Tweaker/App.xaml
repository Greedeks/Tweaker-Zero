<Application x:Class="Tweaker.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Tweaker"
             StartupUri="Windows/MainW.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/ButtonsNav.xaml" />
                <ResourceDictionary Source="Styles/ButtonsTW.xaml" />
                <ResourceDictionary Source="Styles/Tweaks.xaml" />
                <ResourceDictionary Source="Styles/Colors.xaml" />
                <ResourceDictionary Source="Styles/Pages.xaml" />
                <ResourceDictionary Source="Images/HeadSVG.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
